{
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "givenName": {
      "type": "string",
      "contextUri": "https://example.com/Text"
    },
    "familyName": {
      "type": "string"
    },
    "address": {
      "type": "string"
    },
    "amount": {
      "type": "number"
    },
    "age": {
      "type": "integer"
    },
    "weight": {
      "type": "number"
    },
    "height": {
      "type": "number"
    },
    "birth": {
      "type": "string",
      "pattern": "^\\d{4}-\\d{2}-\\d{2}T\\d{2}%3A\\d{2}%3A\\d{2}(?:%2E\\d+)?[A-Z]?(?:[+.-](?:08%3A\\d{2}|\\d{2}[A-Z]))?$"
    },
    "graduated": {
      "type": "string",
      "pattern": "^\\d{4}-(0[1-9]|1[012])-(0[1-9]|[12][0-9]|3[0-1])$"
    },
    "gender": {
      "type": "string",
      "enum": [
        "male",
        "female"
      ]
    },
    "employed": {
      "type": "boolean"
    },
    "geo": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "attitude": {
          "type": "object",
          "additionalProperties": false,
          "properties": {
            "x": {
              "type": "integer"
            },
            "y": {
              "type": "integer"
            }
          },
          "required": [
            "x",
            "y"
          ]
        }
      },
      "required": [
        "attitude"
      ]
    },
    "skills": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "binary": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "binary": {
          "type": "string"
        }
      },
      "required": [
        "binary"
      ]
    },
    "proof": {
      "type": "object",
      "additionalProperties": true
    }
  },
  "required": [
    "givenName",
    "familyName",
    "address",
    "amount",
    "age",
    "weight",
    "height",
    "birth",
    "graduated",
    "gender",
    "employed",
    "geo",
    "skills",
    "binary",
    "proof"
  ],
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}
